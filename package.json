{
	"name": "adopet",
	"version": "1.0.0",
	"main": "server.js",
	"repository": "git@github.com:LucasAlvsz/projeto22-semnome-back.git",
	"author": "Lucas Alves Rodiruges <contato.lucasalv@gmai.com>",
	"license": "MIT",
	"scripts": {
		"prisma:deploy": "prisma migrate deploy",
		"build": "babel src --extensions '.js,.ts' --out-dir build --no-copy-ignored",
		"start": "prisma:deploy && node build/server.js",
		"dev": "tsnd --respawn --transpile-only --ignore-watch node_modules --exit-child --cls --rs -r dotenv/config -r tsconfig-paths/register src/server.ts",
		"test": "jest -i --coverage",
		"test:unit": "clear && jest -i --verbose --coverage --detectOpenHandles tests/unit",
		"test:integration": "clear && jest --i --verbose --coverage --detectOpenHandles tests/integration"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	},
	"dependencies": {
		"@prisma/client": "^4.6.1",
		"axios": "^0.27.2",
		"bcrypt": "^5.0.1",
		"cors": "^2.8.5",
		"express": "^4.18.1",
		"express-async-errors": "^3.1.1",
		"joi": "^17.6.0",
		"jsonwebtoken": "^8.5.1",
		"socket.io": "^4.5.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.9",
		"@babel/core": "^7.18.9",
		"@babel/node": "^7.18.9",
		"@babel/preset-env": "^7.18.9",
		"@babel/preset-typescript": "^7.18.6",
		"@faker-js/faker": "^7.3.0",
		"@types/axios": "^0.14.0",
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/jest": "^28.1.6",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/node": "^18.7.2",
		"@types/socket.io": "^3.0.2",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"babel-plugin-module-resolver": "^4.1.0",
		"dotenv": "^16.0.1",
		"eslint": "^8.20.0",
		"jest": "^28.1.3",
		"prisma": "4.1.1",
		"supertest": "^6.2.4",
		"ts-jest": "^28.0.7",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.0.0",
		"typescript": "^4.7.4"
	}
}
